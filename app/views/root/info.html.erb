<%= render :partial => 'form'%>
<div id="answer">
  <h1><%= @entry.address %></h1>
  <p><%= image_tag(@chart)  %></p>
  <table>
    <tr>
      <th> time </th>
      <th> url referer </th>
      <th> user agent </th>
    </tr>
    <% @entry.views.each do |view|  %>
    <tr>
      <td> <%= view.created_at.strftime('%d %b %Y - %H:%M') %> </td>
      <td> <%= link_to(view.referrer,view.referrer) %> </td>
      <td> <%= view.user_agent %> </td>
    </tr>
    <% end %>
  </table>
</div>
